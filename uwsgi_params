upstream django {
    server unix:///home/ubuntu/backend/mysite.sock; 
}

server {
    listen      8080;
    server_name example.com;
    charset     utf-8;

    client_max_body_size 75M; 

    location /media  {
        alias /home/ubuntu/backend/media; 
    }

    location /static {
        alias /home/ubuntu/backend/static;
    }

    location / {
        uwsgi_pass  django;
        include     /home/ubuntu/backend/uwsgi_params; 
    }
}